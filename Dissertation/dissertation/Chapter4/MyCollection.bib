journal = {Proc. - IEEE Symp. Secur. Priv.},
pages = {334--348},
title = {{Privacy-preserving ridge regression on hundreds of millions of records}},
year = {2013}
}
@article{Machanavajjhala2017,
abstract = {Differential privacy has emerged as an important standard for privacy preserving computation over databases contain-ing sensitive information about individuals. Research on differential privacy spanning a number of research areas, in-cluding theory, security, database, networks, machine learn-ing, and statistics, over the last decade has resulted in a vari-ety of privacy preserving algorithms for a number of analysis tasks. Despite maturing research efforts, the adoption of dif-ferential privacy by practitioners in industry, academia, or government agencies has so far been rare. Hence, in this tutorial, we will first describe the foundations of differen-tially private algorithm design that cover the state of the art in private computation on tabular data. In the second half of the tutorial we will highlight real world applications on complex data types, and identify research challenges in applying differential privacy to real world applications.},
author = {Machanavajjhala, Ashwin and He, Xi and Hay, Michael},
doi = {10.1145/3035918.3054779},
file = {:G$\backslash$:/My Drive/Research/NSF{\_}CICI/Research{\_}Papers/Differential Privacy in the Wild.pdf:pdf},
